{
  "name": "loggylogger",
  "author": "ImTheSpyke",
  "private": false,
  "version": "1.2.1",
  "description": "Fully customizable Logger module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "public",
    "README.md"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "pub": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ImTheSpyke/package-loggylogger.git"
  },
  "keywords": [
    "loggy",
    "logger",
    "loggylogger",
    "imthespyke",
    "spyke"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ImTheSpyke/package-loggylogger/issues"
  },
  "homepage": "https://github.com/ImTheSpyke/package-loggylogger#readme",
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.2",
    "@types/ws": "^8.18.1",
    "@vitest/ui": "^4.0.15",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "express": "^5.2.1",
    "util": "^0.12.5",
    "ws": "^8.18.3"
  }
}
